Main:
	addi $a0, $0, 0x100	
	addi $a1, $0, 0		
	addi $t0, $0, 3
	addi $t1, $0, 50
	addi $s0, $0, 0
	addi $v0, $0, 0

arrayLoop:
	slt $t2, $a1, $t1
	beq $t2, $0, exitLoop
	sll $t2, $a1, 2		
	add $t2, $t2, $a0
	mult $a1, $t0
	mflo $t3
	sw $t3, 0($t2)
	addi $a1, $a1, 1
	j arrayLoop

exitLoop:
	lw $t0, 100($a0)	
	lw $t1, 120($a0)
	addi $t2, $0, 30
	add $t0, $t0, $t1
	div $t0, $t2
	mflo $a0
	sw $a0, 0x00($0)		

	jal factorial

	
	sw $v0, 0x10($0)		
	add $s0, $v0, $0	

factorial:
	addi $sp, $sp, -8
	sw $a0, 4($sp)
	sw $ra, 0($sp)
	addi $t0, $0, 2
	slt $t0, $a0, $t0
	beq $t0, $0, else
	jr $ra
else:
	addi $a0, $a0, -1
	jal factorial
	lw $a0, 4($sp)
	lw $ra, 0($sp)
	addi $sp, $sp, 8
	mult $s0, $v0
	mflo $v0
	jr $ra
	

	
